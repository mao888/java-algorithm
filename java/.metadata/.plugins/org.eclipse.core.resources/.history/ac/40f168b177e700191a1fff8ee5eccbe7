//2
public class Guoqing训练 {

	public static void main(String[] args) {
		System.out.println("请用户输入一个四位数");
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int qianwei = a/1000; 
		int baiwei = a/100%10; 
		int shiwei = a/10%10;
		int gewei = a%10; 
		int b = qianwei+baiwei*10+shiwei*100+gewei*1000;
		System.out.println(b);
		}
	}



















